{
  "name": "api",
  "description": "FeathersJS based API for NASA's MADI infrastructure",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 20.9.0"
  },
  "feathers": {
    "language": "js",
    "packager": "npm",
    "database": "postgresql",
    "framework": "koa",
    "transports": [
      "rest"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "main": "lib/index",
  "type": "module",
  "scripts": {
    "start": "node src",
    "prettier": "npx prettier \"**/*.js\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --recursive --exit",
    "test": "cross-env NODE_ENV=test npm run migrate && npm run mocha",
    "bundle:client": "npm pack --pack-destination ./public",
    "migrate": "knex migrate:latest --debug",
    "migrate:make": "knex migrate:make",
    "seed:admin": "knex seed:run --specific=dev-admin.js",
    "dev:db": "docker-compose up -d database",
    "dev:storage": "docker-compose up -d storage",
    "dev:src": "nodemon src/",
    "dev": "concurrently \"npm run dev:db\" \"npm run dev:storage\" \"npm run dev:src\" ",
    "foas:generate": "npx foas generate",
    "test:contract": "npx newman run ./specifications/contract.postman_collection.json --folder setup --folder chunks",
    "spec:build": "npm run spec:generate; npm run spec:merge; npm run spec:validate; npm run spec:swagger",
    "spec:merge": "npx foas merge",
    "spec:generate": "npx foas generate",
    "spec:validate": "npx @redocly/cli lint --extends=minimal ./specifications/openapi/_merged.yml",
    "spec:mock": "npx @stoplight/prism-cli mock ./specifications/openapi/_merged.yml",
    "spec:swagger": "node ./node_modules/feathers-oas/node_modules/swagger-to-static/index.js ./specifications/openapi/_merged.yml ./public",
    "bru-yml": "bru run ./src/services/chats/chats.spec.yml"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@feathersjs/adapter-commons": "^5.0.11",
    "@feathersjs/authentication": "^5.0.11",
    "@feathersjs/authentication-client": "^5.0.11",
    "@feathersjs/authentication-local": "^5.0.11",
    "@feathersjs/authentication-oauth": "^5.0.11",
    "@feathersjs/configuration": "^5.0.11",
    "@feathersjs/errors": "^5.0.11",
    "@feathersjs/feathers": "^5.0.21",
    "@feathersjs/knex": "^5.0.11",
    "@feathersjs/koa": "^5.0.11",
    "@feathersjs/schema": "^5.0.11",
    "@feathersjs/transport-commons": "^5.0.11",
    "@feathersjs/typebox": "^5.0.11",
    "@google-cloud/logging-winston": "^6.0.0",
    "@google-cloud/storage": "^7.7.0",
    "@koa/multer": "^3.0.2",
    "@sinclair/typebox": "^0.32.13",
    "dotenv": "^16.3.1",
    "feathers-blob": "^2.6.0",
    "feathers-casl": "^2.1.1",
    "feathers-hooks-common": "^8.1.2",
    "feathers-rpc": "^1.0.5",
    "flat": "^6.0.1",
    "fs-blob-store": "^6.0.0",
    "js-yaml": "^4.1.0",
    "knex": "^3.0.1",
    "koa-mount": "^4.0.0",
    "koa-qs": "^3.0.0",
    "langchain": "^0.1.25",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.20.0",
    "p-map": "^7.0.1",
    "pg": "^8.11.3",
    "pgvector": "^0.1.7",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@endo/compartment-mapper": "^1.0.1",
    "@endo/static-module-record": "^1.0.1",
    "@feathersjs/cli": "^5.0.11",
    "@feathersjs/rest-client": "^5.0.11",
    "@stoplight/prism-cli": "^5.5.2",
    "@tootallnate/quickjs-emscripten": "^0.23.0",
    "axios": "^1.6.2",
    "chai": "^5.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.11",
    "feathers-oas": "^0.0.2",
    "isolated-vm": "^4.6.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "patch-package": "^8.0.0",
    "prettier": "^3.1.0",
    "prism": "^1.0.0",
    "ses": "^1.0.1",
    "stacktracey": "^2.1.8"
  },
  "nodemonConfig": {
    "ignoreRoot": [
      ".git"
    ]
  }
}
